{
  "name": "kaluuba-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/server.js",
    "start": "NODE_ENV=production node src/server.js",
    "staging": "NODE_ENV=staging node src/server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:setup": "npm run migrate && npm run seed",
    "db:reset": "npm run migrate:undo:all && npm run migrate && npm run seed",
    "make:migration": "npx sequelize-cli migration:generate --name",
    "make:seed": "npx sequelize-cli seed:generate --name",
    "health": "curl -f http://localhost:${PORT:-3030}/health || exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@sequelize/postgres": "^7.0.0-alpha.46",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "sequelize": "^6.37.7"
  }
}
